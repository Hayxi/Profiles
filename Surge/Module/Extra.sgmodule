[URL Rewrite]
# Unlock AdGuard Premium
^https?:\/\/mobile-api\.adguard\.com\/api\/.+\/ios_validate_receipt$ https://cdn.jsdelivr.net/gh/langkhach270389/Surge-LK@main/scripts/langkhach/adguard.js 302

# Redirect NGA to bbs.nga.cn
^https?:\/\/nga\.178\.com https://bbs.nga.cn 302
^https?:\/\/ngabbs\.com https://bbs.nga.cn 302
^https?:\/\/g\.nga\.cn https://bbs.nga.cn 302
^https?:\/\/yues\.org https://bbs.nga.cn 302

# Redirect Wikipedia to Wikiwand
^https?:\/\/zh\.(m.)?wikipedia\.org\/zh(-\w*)? https://www.wikiwand.com/zh-hans 302
^https?:\/\/(\w*)(-\w*)?(-\w*)?\.(m.)?wikipedia\.org\/wiki https://www.wikiwand.com/$1$2$3 302

# Redirect URL to YouTube App
^https?:\/\/m\.youtube\.com\/watch\?v\= youtube:// 302
^https?:\/\/m\.youtube\.com\/playlist\?list\= youtube:///playlist?list= 302
^https?:\/\/m\.youtube\.com\/channel\/ youtube://www.youtube.com/channel/ 302

